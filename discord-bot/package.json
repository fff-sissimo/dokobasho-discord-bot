{
  "name": "discord-bot",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "deploy-commands": "node deploy-commands.js",
    "start-scheduler": "node scheduler.js",
    "test": "jest --coverage"
  },
  "dependencies": {
    "chrono-node": "^2.9.0",
    "date-fns": "^4.1.0",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5",
    "google-auth-library": "^10.5.0",
    "googleapis": "^170.0.0",
    "node-cron": "^4.2.1",
    "pino": "^10.1.1",
    "pino-pretty": "^13.1.3",
    "uuid": "^8.3.2"
  },
  "optionalDependencies": {
    "@fff-sissimo/fairy-core": "0.1.0"
  },
  "devDependencies": {
    "jest": "^29.7.0"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "index.js",
      "scheduler.js",
      "src/**/*.js",
      "!src/logger.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "__tests__"
    ]
  }
}
